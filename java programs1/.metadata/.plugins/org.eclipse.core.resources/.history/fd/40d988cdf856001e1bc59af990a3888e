package strngs;
import java.util.Scanner;
public class youtube2 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String s=in.nextLine();
		String s1 =" ";
		int count=0;
		for(int i=0;i<s.length()-1;i++) {
			if(s.equals(s1)) {
				count++;
			}
		}
		System.out.println(count);
		
	}

}
