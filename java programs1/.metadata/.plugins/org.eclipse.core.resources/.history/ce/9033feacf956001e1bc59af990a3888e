package strngs;
import java.util.Scanner;
public class youtube2 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String s=in.nextLine();
		int count=0;
		String s1=" ";
		for(int i=0;i<s.length()-1;i++) {
			char c = s.charAt(i);
		  if(c==' ') {
				count++;
			}
		}
		System.out.println(count);
		
	}

}
