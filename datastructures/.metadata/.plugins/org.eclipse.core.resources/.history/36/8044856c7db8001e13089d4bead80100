package com.LL;

public class LL {
   private Node head;
   private Node tail;
   private int size;
   
   public void insertFirst(int val) {
	   Node n = new Node(val);
	   n.next=head;
	   head=n;
	   if(tail==null) {
		   tail=head;
	   }
	   size++;
   }
   public int size() {
	   return size;
   }
   
}
